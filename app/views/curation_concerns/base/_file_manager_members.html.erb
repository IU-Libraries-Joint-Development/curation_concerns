<ul id="sortable" data-id="<%= @presenter.id %>"
    data-class-name="<%= @presenter.model_name.plural %>"
    data-singular-class-name="<%= @presenter.model_name.singular %>"
    data-save-action="<%= polymorphic_path([main_app, @presenter], format: :json) %>"
    class="list-unstyled grid clearfix">
 <% @presenter.member_presenters.each do |member| %>
   <%= render "file_manager_member", node: member %>
 <% end %>
</ul>
